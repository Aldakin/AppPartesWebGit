            if (indice === -1) return; // Esto es cuando no hay elementos
            const optionSelec = $nivel6.options[indice];
            document.getElementById('lbNivel7').style.display = 'none';
            document.getElementById('nivel7').style.display = 'none';
            for (let i = $nivel7.options.length; i >= 0; i--) {
                $nivel7.remove(i);
            }
            div = document.getElementById('information1');
            div.style.display = 'none';
            div = document.getElementById('information2');
            div.style.display = 'none';
            div = document.getElementById('information3');
            div.style.display = 'none';
            div = document.getElementById('information4');
            div.style.display = 'none';
            if (optionSelec.value == 0) {
                alert("Seleccione una opcion correcta o recargue la pagina y vuelva a empezar");
                return;
            }
            var url = "@Url.Action("ObtenerNivel","Main")";
            var cantidad = optionSelec.value;
            var data = { cantidad: cantidad};
            $.post(url, data).done(function (data) {
                //si la operacion es buena
                if (data.length > 1) {
                    document.getElementById('lbNivel7').style.display = 'block';
                    document.getElementById('nivel7').style.display = 'block';
                    const option = document.createElement('option');
                    option.value = "0";
                    option.text = "";
                    $nivel7.appendChild(option);
                    for (let i = 0; i <= data.length - 1; i++) {
                        const option = document.createElement('option');
                        option.value = JSON.stringify(data[i].iValue);
                        option.text = JSON.stringify(data[i].strText).replace(/['"]+/g, '');
                        $nivel7.appendChild(option);
                    }
                } else {
                    if (data.length == 1) {
                    document.getElementById('lbNivel7').style.display = 'block';
                    document.getElementById('nivel7').style.display = 'block';
                    const option = document.createElement('option');
                    option.value = "0";
                    option.text = "";
                    $nivel7.appendChild(option);
                    for (let i = 0; i <= data.length - 1; i++) {
                        const option = document.createElement('option');
                        option.value = JSON.stringify(data[i].iValue);
                        option.text = JSON.stringify(data[i].strText).replace(/['"]+/g, '');
                        $nivel7.appendChild(option);
                        }
                    }
                    else {
                        div = document.getElementById('information1');
                        div.style.display = '';
                        div = document.getElementById('information2');
                        div.style.display = '';
                        div = document.getElementById('information3');
                        div.style.display = '';
                        div = document.getElementById('information4');
                        div.style.display = '';
                    }
                }
                document.getElementById("strPreslin").value = optionSelec.value;
            }).fail(daError).always(function () {
                //esto se ejecuta siempre
            });
        }
